{
  "ConstraintSets": {
    "start": {
      "header": {
        "width": "match_parent",
        "height": "300dp"
      },
      "body": {
        "width": "match_parent",
        "height": "0dp",
        "top": ["header", "bottom", "0dp"],
        "bottom": ["parent", "bottom", "0dp"]
      },
      "content1": {
        "start": ["header", "start"],
        "end": ["header", "end"],
        "top": ["header", "top", "24dp"],
        "bottom": ["content2", "top", "12dp"],
        "height": "0dp"
      },
      "content2": {
        "start": ["header", "start"],
        "end": ["header", "end"],
        "bottom": ["header", "bottom", "24dp"]
      }
    },
    "end": {
      "header": {
        "height": "60dp"
      },
      "body": {
        "width": "match_parent",
        "height": "0dp",
        "top": ["header", "bottom", "0dp"],
        "bottom": ["parent", "bottom", "0dp"]
      },
      "content1": {
        "start": ["header", "start", "24dp"],
        "top": ["header", "top", "8dp"],
        "bottom": ["header", "bottom", "8dp"],
        "height": "0dp"
      },
      "content2": {
        "start": ["content1", "end", "12dp"],
        "bottom": ["header", "bottom"],
        "top": ["header", "top"]
      }
    }
  },
  "Transitions": {
    "transition": {
      "constraintSetStart": "@id/start",
      "constraintSetEnd": "@id/end",
      "duration": 300,
      "keyFrames": {
        "header": {
          "percentX": [0, 1],
          "percentY": [0, 1],
          "alpha": [1, 0]
        },
        "content1": {
          "percentX": [0, 1],
          "percentY": [0, 1],
          "alpha": [1, 0]
        },
        "content2": {
          "alpha": [0, 1]
        }
      }
    }
  }
}